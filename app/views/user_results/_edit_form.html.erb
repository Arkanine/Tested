<%= simple_form_for @user_result do |f|  %>

	<% @user_result.test.questions.each do |question| %>
	    <%= f.simple_fields_for question do |q| %>

            <%= q.input :name, collection: (question.answers.map{|c| c.name}), :as => :radio_buttons %>

	    <% end %>       

	<% end %> <br>

	<%= f.submit %>
<% end %>